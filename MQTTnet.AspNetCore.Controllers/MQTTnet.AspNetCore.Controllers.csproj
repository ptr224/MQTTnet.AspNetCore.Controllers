    <AssemblyVersion></AssemblyVersion>
    <FileVersion></FileVersion>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <Authors>LambdaCom</Authors>
    <Copyright>Copyright (c) LambdaCom 2022</Copyright>
    <Version>2.7.0</Version>
    <RepositoryUrl>https://github.com/ptr224/MQTTnet.AspNetCore.Controllers</RepositoryUrl>
